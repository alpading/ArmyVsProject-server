{
  "dependencies": {
    "aws-sdk": "^2.1569.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "dotenv-cli": "^11.0.0",
    "node-pg-migrate": "^8.0.4"
  },
  "scripts": {
    "migrate:create": "node-pg-migrate create -m migrations",
    "migrate:up": "npx dotenv -e .env -- node-pg-migrate up -m migrations",
    "migrate:down": "npx dotenv -e .env -- node-pg-migrate down -m migrations",
    "migrate:redo": "npx dotenv -e .env -- node-pg-migrate redo -m migrations"
  }
}
