{
    "dependencies": {
        "aws-sdk": "^2.1569.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-session": "^1.18.0",
        "pg": "^8.11.3",
        "pino": "^10.3.1",
        "pino-http": "^11.0.0",
        "pino-pretty": "^13.1.3"
    },
    "devDependencies": {
        "@eslint/js": "^10.0.1",
        "dotenv-cli": "^11.0.0",
        "eslint": "^10.0.0",
        "eslint-config-prettier": "^10.1.8",
        "globals": "^17.3.0",
        "nanoid": "^5.1.6",
        "node-pg-migrate": "^8.0.4",
        "prettier": "^3.8.1"
    },
    "scripts": {
        "migrate:create": "node-pg-migrate create -m migrations",
        "migrate:up": "npx dotenv -e .env -- node-pg-migrate up -m migrations",
        "migrate:down": "npx dotenv -e .env -- node-pg-migrate down -m migrations",
        "migrate:redo": "npx dotenv -e .env -- node-pg-migrate redo -m migrations",
        "format": "prettier --write .",
        "format:staged": "git diff --name-only --cached | grep -E '\\.(js|json|md|yml|yaml)$' | grep -v '^src/client/' | xargs -r npx prettier --write",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "precommit": "npm run format:staged && git add -A && npm run lint"
    }
}
